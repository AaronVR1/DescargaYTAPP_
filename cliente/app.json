{
  "expo": {
    "name": "DescargaYT",
    "slug": "descarga-ytapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a0033"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aaron_v888.descargayt"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a0033"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aaron_v888.descargayt",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c003df3e-da02-4c80-b5b0-f351110f20fa"
      }
    }
  }
}